<?xml version="1.0"?>
<project>
    <info>
        <name>Voter Example</name>
        <version>2.0</version>
        <description>Demonstrates call-in vote counting scheme.</description>
    </info>
    <database>
        <schemas>
            <schema path='ddl.sql' />
        </schemas>
        <procedures>
            <procedure class='com.procedures.Initialize' />
            <procedure class='com.procedures.Results' />
            <procedure class='com.procedures.Vote' />
            <procedure class='com.procedures.ContestantResultsByAreaCode' />
            <procedure class='com.procedures.ContestantWinningStates' />
            <procedure class="com.procedures.VotesByAreaCode">
                <sql>select * from v_votes_by_area_code</sql>
            </procedure>
        </procedures>
        <partitions>
            <partition table='votes' column='phone_number' />
        </partitions>
    </database>
</project>
