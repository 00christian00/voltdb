<?xml version="1.0"?>
<deployment>
    <cluster hostcount="1" sitesperhost="2" kfactor="0" />
    <httpd enabled="true">
        <jsonapi enabled="true" />
    </httpd>
    <export enabled="true" target="kafka">
        <configuration>
            <property name="dateformat">yyyy-MM-dd-HH-mm-ss</property>
            <property name="metadata.broker.list">localhost:9092,anish:9092,volt6a:9092</property>
            <property name="request.required.acks">1</property>
        </configuration>
    </export>
</deployment>
