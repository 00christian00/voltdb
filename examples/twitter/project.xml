<?xml version="1.0"?>
<project>
    <database name='database'>
        <schemas>
            <schema path='twitter-ddl.sql' />
        </schemas>
        <procedures>
            <procedure class='org.voltdb.twitter.database.procedures.Insert' />
            <procedure class='org.voltdb.twitter.database.procedures.Select' />
            <procedure class='org.voltdb.twitter.database.procedures.Delete' />
        </procedures>
        <partitions>
            <partition table='hashtags' column='hashtag' />
        </partitions>
        <exports>
            <connector class="org.voltdb.elt.processors.RawProcessor" enabled="true">
                <tables>
                    <table name="hashtags" exportonly="false" />
                </tables>
            </connector>
        </exports>
    </database>
</project>