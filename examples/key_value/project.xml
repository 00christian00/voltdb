<?xml version="1.0"?>
<project>
    <info>
        <name>VoltKV</name>
        <version>1.0</version>
        <description>High performance Key/Value data store implementation sample using VoltDB.</description>
    </info>
    <database>
        <schemas>
            <schema path='ddl.sql' />
        </schemas>
        <procedures>
            <procedure class='com.procedures.Initialize' />
            <procedure class='com.procedures.Get' />
            <procedure class='com.procedures.Put' />
            <procedure class='com.procedures.Remove' />
        </procedures>
        <partitions>
            <partition table='store' column='key' />
        </partitions>
    </database>
</project>
